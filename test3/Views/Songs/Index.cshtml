@model IEnumerable<test3.Song>

@{
    ViewBag.Title = "ShaneTopSongs";
}

<h2>Shane's Top Five Songs</h2>


<table class="table">
    <tr>
        <th>
            Song 
        </th>
        <th>
           Artist
        </th>
        <th>
            Album
        </th>
        <th>
           Link
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Sname)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SArtistName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SAlbumName)
        </td>
        <td>
            <a href="@Html.DisplayFor(modeItem => item.SYoutubeLink)" target="_blank">YouTube</a>
          
        </td>
       
    </tr>
}

</table>
